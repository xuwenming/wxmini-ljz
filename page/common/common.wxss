.flex
{
	display: -webkit-box !important;
	display: box !important;
}

.flex-row
{
	-webkit-box-flex: 1; 
	box-flex: 1;
}

.flex-ver {
	-webkit-box-orient: vertical;
}

/*水平方向 居左*/
.flex-pack-start {
	-webkit-box-pack: start;
}

/*水平方向 居右*/
.flex-pack-end {
	-webkit-box-pack: end;
}

/*水平方向 居中*/
.flex-pack-center {
	-webkit-box-pack: center;
}

/*垂直方向 居上*/
.flex-align-start {
	-webkit-box-align: start;
}

/*垂直方向 居下*/
.flex-align-end {
	-webkit-box-align: end;
}

/*垂直方向 居中*/
.flex-align-center {
	-webkit-box-align: center;
}

.line-before:before {
    content: " ";
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    height: 1px;
    border-top: 1px solid #D9D9D9;
    color: #D9D9D9;
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
}

.line-after:after {
    content: " ";
    position: absolute;
    left: 0;
    bottom: 0;
    right: 0;
    height: 1px;
    border-bottom: 1px solid #D9D9D9;
    color: #D9D9D9;
    -webkit-transform-origin: 0 100%;
    transform-origin: 0 100%;
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
}

.line-after-bl:after {
    content: " ";
    position: absolute;
    left: 0;
    bottom: 0;
    right: 0;
    height: 1px;
    border-bottom: 1px solid #D9D9D9;
    color: #D9D9D9;
    -webkit-transform-origin: 0 100%;
    transform-origin: 0 100%;
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
    left: 12px;
}

.line:first-child:before {
    display: none;
}

